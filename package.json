{
  "name": "nest-swe",
  "version": "0.0.1",
  "description": "NestJS SWE Template",
  "author": "n1md7",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=debug nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test vitest run",
    "test:ci": "npm test -- --reporter junit --outputFile reports/junit.xml --coverage",
    "test:watch": "cross-env NODE_ENV=test vitest",
    "test:cov": "cross-env NODE_ENV=test vitest run --coverage",
    "test:debug": "cross-env NODE_ENV=test vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "cross-env NODE_ENV=e2e vitest run --config ./vitest.config.e2e.mts",
    "gen:env-files": "node scripts/gen-envs.cjs",
    "gen:env-types": "npx gen-env-types .env.example -o src/env.d.ts -k -e .",
    "gen:docs": "npx compodoc -p tsconfig.doc.json --output public",
    "prepare": "husky install",
    "dbg": "NEST_DEBUG=true npm run start:dev"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.1",
    "@nestjs/platform-express": "^10.3.1",
    "@nestjs/swagger": "^7.2.0",
    "@sentry/node": "^7.99.0",
    "@sentry/types": "^7.99.0",
    "axios": "^1.6.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.1",
    "joi": "^17.12.1",
    "ms": "^2.1.3",
    "node-cache": "^5.1.2",
    "pino": "^8.19.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.23",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.1",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.107",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.11.16",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@types/validator": "^13.11.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "gen-env-types": "^1.3.4",
    "husky": "^9.0.10",
    "js-yaml": "^4.1.0",
    "prettier": "^3.2.4",
    "supertest": "^6.3.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "unplugin-swc": "^1.4.4",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": "20.10.0",
    "npm": ">=10.0.0"
  },
  "engineStrict": true
}
